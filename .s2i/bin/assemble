#!/bin/sh

SRCCONF=/tmp/src/configuration
SRCLIB=/tmp/src/lib
SRCBIN=/tmp/src
SRCPATCH=/tmp/src/patch

if [ -d ${SRCCONF} ]; then
  echo "Copying config files from project..."
  cp -v ${SRCCONF}/openshift-activemq.xml  $AMQ_HOME/conf/
  cp -v ${SRCCONF}/${environment}-login.config $AMQ_HOME/conf/openshift-login.config
fi
